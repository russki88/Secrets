<%- include('partials/header') %>

<div class="jumbotron text-center">
  <div class="container">
    <i class="fas fa-key fa-6x"></i>
    <h1 class="display-3">Tell the Lies you have told!</h1>

    <% usersWithLies.forEach(function(user){ %>
      <p class="lie-text"><%=user.lie%></p>
    <% }); %>

    <hr>

    <a class="btn btn-light btn-lg" href="/logout" role="button">Log Out</a>
    <a class="btn btn-dark btn-lg" href="/submit" role="button">Submit a Lie</a>
  </div>
</div>

<%- include('partials/footer') %>
